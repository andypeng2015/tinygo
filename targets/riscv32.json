{
	"inherits": ["riscv"],
	"llvm-target": "riscv32-unknown-none",
	"cpu": "generic-rv32",
	"target-abi": "ilp32",
	"build-tags": ["tinygo.riscv32"],
	"scheduler": "tasks",
	"default-stack-size": 2048,
	"cflags": [
		"-march=rv32imac"
	],
	"ldflags": [
		"-melf32lriscv",
		"-mllvm", "-enable-machine-outliner=never"
	],
	"gdb": [
		"gdb-multiarch",
		"gdb"
	]
}
